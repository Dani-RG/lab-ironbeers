<div class="col-md-4">
  <div class="card">
    <img
      class="card-img-top"
      src="{{this.image_url}}"
      alt="{{this.name}}"
    />
    <div class="card-body">
      <h5 class="card-name">{{this.name}}</h5>
      <p class="card-tagline">{{this.tagline}}</p>
      <p class="card-description">{{this.description}}</p>
    </div>
  </div>
</div>